@model Semester_Project.Models.Research
@{
    ViewBag.Title = "AddResearch";
}

 <div class="row">
        <div class="col-lg-9 col-lg-offset-1">
<h2>AddResearch</h2>
            </div>
     </div>
@using (Html.BeginForm("Add", "AdminResearch", FormMethod.Post, new { enctype = "multipart/form-data" }))
{ 
    <div class="row">
        <div class="col-lg-9 col-lg-offset-2">
               <p>
               @*@Html.Label("Enter Name:")*@
               @Html.LabelFor(x => x.Title)
               </p>
           
               @Html.TextBoxFor(x => x.Title)
               @Html.ValidationMessageFor(x => x.Title)

            <br />
                <p>
                    <br />
               @Html.Label("Enter Summery:")
                </p>

               @Html.TextBoxFor(x => x.Summery)
                @Html.ValidationMessageFor(x => x.Summery)
                 <p>
                    <br />
               @Html.Label("Enter Details:")
                </p>
               @Html.TextAreaFor(x => x.Details)
                @Html.ValidationMessageFor(x => x.Details)
            <br />
                @Html.Label("Enter Id of person:")
            <br />
                @Html.TextBoxFor(x => x.People)

            <br />
                <label for="photo">File</label>
                <input type="file" class="file" id="f1" name="file"/>
            <br />
            <div>
            <input type="submit" value="Submit" id="b1" class="btn btn-submit" />
            </div>
        </div>
    </div>
}
